doctype html
html
	head
		link(href="css/news.css", rel="stylesheet", type="text/css")
		title CartaStraccia - RSS Feed Reader
		:css
	body
		#list(class="index")
			h1 Cartastraccia
			h2 "...fuck ads"
			#index(class="index")
			- uint cnt = 0;
			- foreach(feed; validFeeds)
				#chname(class="index")
					p 
						a(href="channels/"~feed.name~".html") #{feed.name}
					br
					a(href=feed.path) external link
